{
  "name": "strangerchat",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "postinstall": "react-native setup-ios-permissions && pod-install",
    "cleaniOSCache": "cd ios && xcodebuild clean -workspace strangerchat.xcworkspace -scheme strangerchat -configuration Debug",
    "watchdel": "watchman watch-del $PWD ; watchman watch-project $PWD"
  },
  "reactNativePermissionsIOS": [
    "Camera",
    "PhotoLibrary"
  ],
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.22.10",
    "@expo/react-native-action-sheet": "^4.0.1",
    "@react-native-async-storage/async-storage": "^1.19.1",
    "@react-native-clipboard/clipboard": "^1.11.2",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/material-top-tabs": "^6.6.3",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.4.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "react": "18.2.0",
    "react-native": "0.72.3",
    "react-native-config": "^1.5.1",
    "react-native-date-picker": "^4.2.13",
