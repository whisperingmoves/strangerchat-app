    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "eslint-plugin-import": "^2.28.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.7",
    "pod-install": "^0.1.38",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
